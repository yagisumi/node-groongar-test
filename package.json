{
  "name": "groongar-test",
  "version": "0.1.0",
  "description": "The purpose of testing for @yagisumi/groongar",
  "keywords": [],
  "engines": {
    "node": ">= 10.0.0"
  },
  "private": true,
  "scripts": {
    "test": "run-s prettier:check eslint jest build check-ts grntest:nroonga",
    "build": "tsc -p test/src",
    "clean": "rimraf lib test/lib tools/lib coverage umd *.tgz .rpt2_cache",
    "check-ts": "tsc --noEmit -p test",
    "grntest:nroonga": "cross-env TS_NODE_FILES=true mocha -r test/lib/mocha_env_nroonga.js \"test/grntest/suite/**/*.test.ts\"",
    "grntest:nroonga-i": "node test/lib/i-test.js",
    "grntest:http": "cross-env TS_NODE_FILES=true mocha -r test/lib/mocha_env_http.js \"test/grntest/suite/**/*.{test,i-test}.ts\"",
    "typecheck": "mocha \"test/typecheck/**/*.test.ts\"",
    "prettier:check": "prettier --check \"**/*.{ts,js,json}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,js,json}\"",
    "eslint": "eslint \"{test,tools}/{src,test}/**/*.{js,ts}\"",
    "tools:test": "cross-env TS_NODE_FILES=true TS_NODE_PROJECT=tools/tsconfig.json mocha --config tools/test/.mocharc.json",
    "tools:usage": "ts-node tools/src/tools.ts",
    "tools:init": "ts-node tools/src/tools.ts init",
    "tools:convert": "ts-node tools/src/tools.ts convert",
    "tools:outputs": "ts-node tools/src/tools.ts outputs",
    "tools:typecheck": "ts-node tools/src/tools.ts typecheck",
    "tools:clean": "ts-node tools/src/tools.ts clean",
    "tools:clean_test": "ts-node tools/src/tools.ts clean_test"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/node-groongar-test",
  "homepage": "http://github.com/yagisumi/node-groongar-test#readme",
  "license": "MIT",
  "prettier": "@yagisumi/my-config-prettier",
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/deep-equal": "^1.0.1",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.41",
    "@types/prettier": "^2.2.3",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@yagisumi/groonga-command": "^0.1.0",
    "@yagisumi/groonga-http-client": "file:../groonga-http-client/yagisumi-groonga-http-client-0.1.0.tgz",
    "@yagisumi/groongar": "file:../groongar/yagisumi-groongar-0.1.0.tgz",
    "@yagisumi/my-config-prettier": "^1.0.0",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "deep-equal": "^2.0.5",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "expect": "^26.6.2",
    "get-port": "^5.1.1",
    "heredocument": "^2.0.2",
    "mkdirp": "^1.0.4",
    "mocha": "^8.3.2",
    "mocha-ui-jest": "^0.4.0",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "nroonga": "^0.4.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sortobject": "^4.14.0",
    "ts-hashcode": "^1.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsd": "^0.14.0",
    "typescript": "^4.2.4"
  }
}
