{
  "name": "groongar-test",
  "version": "0.1.0",
  "description": "For testing purposes",
  "keywords": [],
  "engines": {
    "node": ">= 10.0.0"
  },
  "private": true,
  "scripts": {
    "test": "run-s prettier:fix eslint jest",
    "build": "run-s clean test:build tools:build",
    "test:build": "tsc -p test/src",
    "clean": "rimraf lib test/lib tools/lib coverage umd *.tgz .rpt2_cache",
    "grntest:nroonga": "cross-env TS_NODE_FILES=true mocha -p --reporter list --timeout 30000 -r ts-node/register -r ./test/lib/mocha_env_nroonga \"test/grntest/suite/**/*.test.ts\"",
    "prettier:fix": "prettier --write \"**/*.{ts,js,json}\"",
    "eslint": "eslint \"**/*.{js,ts}\"",
    "tools:test": "cross-env TS_NODE_FILES=true mocha -r ts-node/register \"tools/test/**/*.test.ts\"",
    "tools:usage": "ts-node tools/src/tools.ts",
    "tools:init": "ts-node tools/src/tools.ts init",
    "tools:clean": "ts-node tools/src/tools.ts clean",
    "tools:convert": "ts-node tools/src/tools.ts convert",
    "tools:__build": "tsc -p tools/src"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/node-groongar-test",
  "homepage": "http://github.com/yagisumi/node-groongar-test#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.1",
    "@types/prettier": "^2.1.1",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@yagisumi/groonga-command": "^0.1.0",
    "@yagisumi/groongar": "file:yagisumi-groongar-0.1.0.tgz",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "heredocument": "^2.0.2",
    "mkdirp": "^1.0.4",
    "mocha": "^8.1.3",
    "moment": "^2.28.0",
    "npm-run-all": "^4.1.5",
    "nroonga": "^0.4.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sortobject": "^4.13.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
