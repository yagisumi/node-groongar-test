{
  "name": "groongar-test",
  "version": "0.1.0",
  "description": "",
  "keywords": [],
  "engines": {
    "node": ">= 6.0.0"
  },
  "private": true,
  "main": "lib/groongar-test",
  "module": "lib/groongar-test.mjs",
  "types": "lib/groongar-test.d.ts",
  "unpkg": "umd/groongar-test.js",
  "scripts": {
    "test": "run-s prettier:fix eslint jest",
    "jest": "jest",
    "doc": "typedoc --json ./docs/typedoc.json --tsconfig ./tsconfig.json ./src",
    "build": "run-s clean build:*",
    "clean": "rimraf lib coverage umd *.tgz .rpt2_cache",
    "build:cjs": "rollup -c -f cjs -o ./lib/groongar-test.js",
    "build:mjs": "rollup -c -f es -o ./lib/groongar-test.mjs",
    "build:umd": "rollup -c rollup.config.umd.js",
    "build:d_ts": "tsc -p src -d --emitDeclarationOnly --outDir ./lib",
    "prettier:fix": "prettier --write \"**/*.{ts,js,json}\"",
    "eslint": "eslint \"**/*.{js,ts}\"",
    "prepack": "run-s build test"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/node-groongar-test",
  "homepage": "http://github.com/yagisumi/node-groongar-test#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.27.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.3"
  }
}
